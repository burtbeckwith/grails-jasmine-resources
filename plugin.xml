<plugin name='jasmine-resources' version='0.1.0' grailsVersion='2.2.4 &gt; *'>
  <author>Vladimír Oraný</author>
  <authorEmail>vladimir.orany@appsatori.eu</authorEmail>
  <title>Jasmine Resources Plugin</title>
  <description>Jasmine resources plugin bundled with 1.1.0 release.
</description>
  <documentation>http://grails.org/plugin/jasmine-resources</documentation>
  <type>JasmineResourcesGrailsPlugin</type>
  <resources>
    <resource>ExampleResources</resource>
    <resource>JasmineResourcesPluginResources</resource>
    <resource>JasmineResourcesUrlMappings</resource>
    <resource>jasmine.resources.JasmineController</resource>
  </resources>
  <repositories>
    <repository name='grailsCentral' url='http://grails.org/plugins' />
    <repository name='mavenCentral' url='http://repo1.maven.org/maven2/' />
    <repository name='http://grails.org/plugins' url='http://grails.org/plugins' />
  </repositories>
  <dependencies>
    <runtime>
      <dependency group='mysql' name='mysql-connector-java' version='5.1.20' />
    </runtime>
  </dependencies>
  <plugins>
    <compile>
      <plugin group='org.grails.plugins' name='cache-headers' version='1.1.5' />
      <plugin group='org.grails.plugins' name='compass-sass' version='0.7' />
      <plugin group='org.grails.plugins' name='coffeescript-resources' version='0.3.8' />
      <plugin group='org.grails.plugins' name='closure-compiler' version='0.9.1' />
      <plugin group='org.grails.plugins' name='spring-security-core' version='1.2.7.3' />
      <plugin group='org.grails.plugins' name='cache' version='1.0.1' />
      <plugin group='org.grails.plugins' name='jquery-ui' version='1.8.24' />
    </compile>
    <runtime>
      <plugin group='org.grails.plugins' name='zipped-resources' version='1.0' />
      <plugin group='org.grails.plugins' name='jquery' version='1.8.3' />
      <plugin group='org.grails.plugins' name='hibernate' version='2.2.4' />
      <plugin group='org.grails.plugins' name='database-migration' version='1.3.3' />
      <plugin group='org.grails.plugins' name='resources' version='1.2' />
      <plugin group='org.grails.plugins' name='cached-resources' version='1.0' />
    </runtime>
    <build>
      <plugin group='org.grails.plugins' name='tomcat' version='2.2.4' />
    </build>
  </plugins>
  <runtimePluginRequirements />
  <behavior />
</plugin>